<h1>Add A New Director</h1>

<form action="http://localhost:3000/create_director">
  <div>
    <label for="director_name">Director:</label>
    <input id="director_name" type="text" name="the_name">
  </div>
  <div>
    <label for="director_dob">D.O.B.:</label>
    <input id="director_dob" type="text" name="the_dob">
  </div>
  <div>
    <label for="director_bio">Biography:</label>
    <input id="director_bio" type="text" name="the_bio">
  </div>
  <div>
    <label for="director_image_url">Image:</label>
    <input id="director_image_url" type="text" name="the_source">
  </div>
  <div>
    <button>Create Director</button>
  </div>
</form>
